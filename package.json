{
    "name": "sourcegraph-chrome-extension",
    "version": "0.0.1",
    "description": "Sourcegraph browser extension for Chrome",
    "scripts": {
        "dev": "NODE_ENV=development node scripts/dev",
        "build": "NODE_ENV=production node scripts/build",
        "graphql": "get-graphql-schema http://localhost:3080/.api/graphql --json | gql2ts -o app/gqlschema.d.ts",
        "lint": "npm run tslint && npm run stylelint",
        "tslint": "tslint -c tslint.json -p tsconfig.json 'app/**/*.ts?(x)'",
        "stylelint": "stylelint 'app/**/*.scss'",
        "clean": "rm -rf dist/ *.zip *.xpi .checksum"
    },
    "author": "John Rothfels <john@sourcegraph.com>",
    "license": "MIT",
    "devDependencies": {
        "@sourcegraph/tslint-config": "^7.0.0",
        "@types/bluebird": "^3.0.35",
        "@types/lodash": "^4.14.72",
        "@types/marked": "^0.0.28",
        "@types/node": "^8.0.20",
        "@types/query-string": "^4.3.1",
        "@types/react": "^16.0.2",
        "@types/reactstrap": "^5.0.18",
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-new-target": "^7.0.0-alpha.14",
        "babel-preset-env": "^1.6.1",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^3.0.2",
        "get-graphql-schema": "^2.1.1",
        "gql2ts": "^1.2.1",
        "jade": "^1.11.0",
        "node-sass": "^4.5.3",
        "postcss-loader": "^2.0.8",
        "prettier": "^1.7.4",
        "sass-loader": "^6.0.6",
        "shelljs": "^0.7.8",
        "style-loader": "^0.18.2",
        "stylelint": "^8.2.0",
        "stylelint-config-prettier": "^1.0.2",
        "stylelint-config-recommended": "^1.0.0",
        "ts-loader": "^2.3.6",
        "tslint": "^5.7.0",
        "tslint-language-service": "^0.9.6",
        "tslint-webpack-plugin": "^1.0.0",
        "typescript": "^2.5.2",
        "web-ext": "^2.5.0",
        "webpack": "^3.11.0"
    },
    "dependencies": {
        "@sourcegraph/icons": "^1.7.0",
        "@sourcegraph/telligent-tracker": "^0.1.6",
        "@types/filesystem": "0.0.28",
        "autobind-decorator": "^1.3.4",
        "bluebird": "^3.3.4",
        "bootstrap": "^4.0.0",
        "formdata-polyfill": "^3.0.9",
        "highlight.js": "^9.12.0",
        "lodash": "^4.17.4",
        "marked": "^0.3.6",
        "query-string": "^4.3.4",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-icon-base": "^2.0.0",
        "react-router-dom": "^4.2.2",
        "react-transition-group": "^2.2.1",
        "reactstrap": "^5.0.0-beta.2",
        "rxjs": "^5.5.2",
        "vscode-languageserver-types": "^3.4.0",
        "whatwg-fetch": "^2.0.3",
        "whatwg-url": "^6.2.1"
    }
}
