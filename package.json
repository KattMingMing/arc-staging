{
  "name": "sourcegraph-chrome-extension",
  "version": "0.0.1",
  "description": "Sourcegraph browser extension for Chrome",
  "scripts": {
    "test": "NODE_ENV=test mocha --opts mocha.opts",
    "test:auto": "NODE_ENV=test mocha --opts mocha.opts --watch",
    "dev": "NODE_ENV=development node scripts/dev",
    "build": "NODE_ENV=production node scripts/build",
    "lint": "./node_modules/.bin/tslint -c tslint.json -e 'node_modules/**' '**/*.tsx'",
    "lint-fix": "./node_modules/.bin/tslint -c tslint.json -e 'node_modules/**' '**/*.tsx' --fix",
    "fmt": "./node_modules/.bin/tsfmt -r",
    "fmt-check": "./node_modules/.bin/tsfmt --verify",
    "clean": "rm -rf dist/ *.zip *.xpi .checksum"
  },
  "author": "John Rothfels <john@sourcegraph.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chrome": "^0.0.48",
    "@types/jquery": "^3.2.10",
    "@types/jsdom": "^2.0.30",
    "@types/lodash": "^4.14.72",
    "@types/marked": "^0.0.28",
    "@types/node": "^8.0.20",
    "@types/query-string": "^4.3.1",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.2",
    "autoprefixer": "^7.1.2",
    "css-loader": "^0.28.4",
    "hash-files": "^1.1.1",
    "jade": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "shelljs": "^0.6.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.1.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "typescript-formatter": "^4.0.1",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "@sourcegraph/rxjs": "^5.4.2",
    "autobind-decorator": "^1.3.4",
    "bluebird": "^3.3.4",
    "highlight.js": "^9.12.0",
    "jquery": "^3.2.1",
    "jquery-pjax": "^2.0.1",
    "jquery-resizable-dom": "^0.20.0",
    "jstree": "^3.3.4",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "query-string": "^4.3.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icon-base": "^2.0.0",
    "telligent-tracker": "^0.1.2",
    "whatwg-fetch": "^2.0.3"
  }
}
