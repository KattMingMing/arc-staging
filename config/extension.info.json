{
    "version": "1.6.6",
    "name": "Sourcegraph for GitHub",
    "author": "Sourcegraph <hi@sourcegraph.com>",
    "description": "Gives GitHub IDE powers: jump to definition and hover tooltips in code, PRs, and diffs",
    "permissions": ["https://github.com/*", "https://localhost:3443/*", "https://sourcegraph.com/*"],
    "backgroundScript": "js/background.bundle.js",
    "injectScript": "js/inject.bundle.js",
    "optionsUI": "options.html",
    "settings": {
        "sourcegraphURL": {
            "type": "string",
            "defaultValue": "https://sourcegraph.com"
        },
        "gitHubEnterpriseURL": {
            "type": "string"
        },
        "phabricatorURL": {
            "type": "string"
        },
        "repositoryFileTreeEnabled": {
            "type": "boolean",
            "defaultValue": true
        },
        "repositorySearchEnabled": {
            "type": "boolean",
            "defaultValue": true
        },
        "sourcegraphRepoSearchToggled": {
            "type": "boolean",
            "defaultValue": false
        },
        "eventTrackingEnabled": {
            "type": "boolean",
            "defaultValue": true
        },
        "openEditorEnabled": {
            "type": "boolean",
            "defaultValue": false
        },
        "identity": {
            "type": "string"
        }
    }
}
